        <Label x:Name="admissaoCol" Content="Data Admissão" HorizontalAlignment="Left" Margin="807,117,0,0" VerticalAlignment="Top" FontFamily="Segoe UI Black"/>
        <DatePicker x:Name="textAdmissaoCol" HorizontalAlignment="Left" Margin="807,143,0,0" VerticalAlignment="Top" Width="164"/>
