            <RadioButton x:Name="CLTCol" Content="CLT" Height="24"/>
            <RadioButton x:Name="EstagioCol" Content="Estágio" Height="18"/>
        </StackPanel>
        <StackPanel x:Name="StackpanelCol2" HorizontalAlignment="Left" Height="69" Margin="810,280,0,0" VerticalAlignment="Top" Width="164">
            <RadioButton x:Name="AdmCol" Content="Administrador" Height="24"/>
            <RadioButton x:Name="atenCol" Content="Atendente" Height="24"/>
            <RadioButton x:Name="instCol" Content="Instrutor"/>
        </StackPanel>
        <Label x:Name="cargoCol" Content="Cargo" HorizontalAlignment="Left" Margin="807,190,0,0" VerticalAlignment="Top" FontFamily="Segoe UI Black"/>

